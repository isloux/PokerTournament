{
	"name": "pokertournament",
	"private": true,
	"homepage": "https://isloux.github.io/pokertournament",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build:wasm": "wasm-pack build src/wasm/ticker --target web --out-dir ../../../src/lib/wasm/ticker-pkg",
		"predev": "npm run build:wasm",
		"dev": "vite dev",
		"prebuild": "npm run build:wasm",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest run --passWithNoTests",
		"test:watch": "vitest",
		"prepare": "svelte-kit sync || echo ''",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build --dotfiles --nojekyll"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"gh-pages": "^6.3.0",
		"jsdom": "^28.1.0",
		"svelte": "^5.51.0",
		"vite": "^7.3.1",
		"vite-plugin-top-level-await": "^1.6.0",
		"vite-plugin-wasm": "^3.5.0",
		"vitest": "^4.0.18"
	}
}
